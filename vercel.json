{
  "version": 2,
  "builds": [
    {
      "src": "server-minimal.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-minimal.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "server-minimal.js": {
      "maxDuration": 10
    }
  }
}